{
  "name": "jps-landing-page",
  "version": "2.0.0",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "build-stats": "cross-env ANALYZE=true npm run build",
    "clean": "rimraf .next out",
    "lint": "next lint",
    "build-types": "tsc --noEmit --pretty",
    "prepare": "husky install",
    "migrate": "tsx scripts/migrate.ts",
    "migrate:roles": "tsx --env-file=.env.local scripts/migrate-roles.ts",
    "db:init": "tsx scripts/migrate.ts",
    "seed:blacklist": "node -r dotenv/config scripts/seed-blacklist.ts dotenv_config_path=.env.local || tsx --env-file=.env.local scripts/seed-blacklist.ts",
    "seed:blacklist:force": "FORCE_SEED=true node -r dotenv/config scripts/seed-blacklist.ts dotenv_config_path=.env.local || FORCE_SEED=true tsx --env-file=.env.local scripts/seed-blacklist.ts",
    "set-role": "tsx --env-file=.env.local scripts/set-user-role.ts"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.18",
    "@heroicons/react": "^2.1.1",
    "@neondatabase/serverless": "^1.0.2",
    "@react-pdf/renderer": "^4.3.2",
    "@sendgrid/mail": "^8.1.6",
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/nodemailer": "^7.0.9",
    "@types/pg": "^8.16.0",
    "bcryptjs": "^3.0.3",
    "clsx": "^2.1.0",
    "date-fns": "^4.1.0",
    "framer-motion": "^11.0.3",
    "jsonwebtoken": "^9.0.3",
    "lucide-react": "^0.563.0",
    "next": "^14.1.0",
    "nodemailer": "^7.0.13",
    "pg": "^8.18.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sharp": "^0.34.5",
    "swiper": "^12.0.1"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^14.1.0",
    "@types/node": "^20.19.30",
    "@types/react": "^18.2.48",
    "@types/react-dom": "^18.2.18",
    "autoprefixer": "^10.4.17",
    "cross-env": "^7.0.3",
    "dotenv": "^17.2.3",
    "eslint": "^8.56.0",
    "eslint-config-next": "^14.1.0",
    "husky": "^8.0.3",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.33",
    "prettier": "^3.2.4",
    "rimraf": "^5.0.5",
    "tailwindcss": "^3.4.1",
    "tsx": "^4.21.0",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": "22.x",
    "npm": "10.x"
  },
  "license": "ISC"
}
